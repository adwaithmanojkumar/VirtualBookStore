package Dsa;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;

public class MajorityElementII {

	public static void main(String[] args) {
		int[] arr= {7,3,3,9,7};
//		System.out.println(majorityElementBrute(arr).get(0)+" "+majorityElementBrute(arr).get(1));
		System.out.println(majorityElementBetter(arr).get(0)+" "+majorityElementBetter(arr).get(1));
	}
	
	//Brute-force
	public static List<Integer> majorityElementBrute(int[] v) {
		int n=v.length;
		List<Integer> list = new ArrayList<>();
		for(int i=0;i<n;i++) {
			int count=1;
			for(int j=i+1;j<n;j++) {
				if(v[j]==v[i])
					count++;
				
				if(count>n/3) {
					list.add(v[i]);
					break;
				}
			}
			if(list.size()==2) break;
		}
		return list;
	}
	
	//Better
	public static List<Integer> majorityElementBetter(int[] v) {
		int n=v.length;
		List<Integer> list = new ArrayList<>();
		HashMap<Integer, Integer> map = new HashMap<>();
		for(int i=0;i<n;i++) {
			
		}
		return list;
	}
}