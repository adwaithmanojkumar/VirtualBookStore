package CP;

import java.util.Scanner;

public class BeautifulMatrix {
	
	public static int main(String[] args) {
		int[][] arr = new int[5][5];
		
		int count=0;
		
		for(int i=0;i<arr.length;i++) {
			for(int j=0;j<arr[i].length;j++) {
				if(arr[i][j]==1) {
					while(i!=2 || j!=2) {
						if(i<2) {
							i++;
							count++;
						} else if(i>2) {
							i--;
							count++;
						}
						if(j<2) {
							j++;
							count++;
						} else if(j>2) {
							j--;
							count++;
						}
					}
					break;
				}
			}
		}
		
		return count;
		
		
	}
}
