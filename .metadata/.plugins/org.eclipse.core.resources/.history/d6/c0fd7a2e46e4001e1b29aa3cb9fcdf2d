package Dsa;

public class SpiralMatrixII {

	public static void main(String[] args) {

	}

	public int[][] get2DMatrix(int A) {
		int rmin = 0;
		int rmax = A;
		int cmin = 0;
		int cmax = A;
		int count = 0;

		if (count < A * A) {
			for (int i = cmin; i < cmax; i++) {
				if(count<A*A)
				count++;
			}
			rmin++;

			for (int i = rmin; i < rmax; i++) {
				count++;
			}
			cmax--;

			for (int i = cmax; i > cmin; i++) {
				count++;
			}
			rmax--;

			for (int i = rmax; i > rmin; i++) {
				count++;
			}
			cmin++;
		}

		return null;
	}

}
