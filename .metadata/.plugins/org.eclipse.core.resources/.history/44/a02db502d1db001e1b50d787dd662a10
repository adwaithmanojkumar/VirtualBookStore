package com.adwaith.juint;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class MathTest {

	@Test
	void test() {
		int[] arr= {1,2,3};
		
		calculationSquare calcsq = new calculationSquare();
		
		int result=calcsq.calculationSquareOfNums(arr);
		int expectedResult=6;
		
		assertEquals(result, expectedResult);
	}

}
