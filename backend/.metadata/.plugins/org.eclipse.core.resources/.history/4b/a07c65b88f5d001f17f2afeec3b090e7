package com.example.ecommerce_website.controller;

import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.example.ecommerce_website.service.RatingService;
import com.example.ecommerce_website.service.UserService;

@RestController
@RequestMapping("/api/ratings")
public class RatingController {
	private UserService uerService;
	private RatingService ratingService;
	
	public RatingController(UserService uerService, RatingService ratingService) {
		super();
		this.uerService = uerService;
		this.ratingService = ratingService;
	}
	
	
}
